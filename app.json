{
  "expo": {
    "name": "Ismarlıyorum Mağaza",
    "slug": "ismarliyorumcafe",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ismarliyorumcafe",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.abidino.ismarliyorumcafe",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FF6620",
        "foregroundImage": "./assets/images/icon.png"
      },
      "package": "com.abidino.ismarliyorumcafe",
      "versionCode": 3,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#FF6620"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Uygulama, sipariş ve QR kodlarını okumak için kameranıza erişim izni istiyor."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "186911a0-97c1-4757-8a70-d6434fbe25b4"
      }
    },
    "owner": "abidino",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/186911a0-97c1-4757-8a70-d6434fbe25b4"
    }
  }
}
